{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AppDbConnectionString": "data source=(LocalDb)\\MSSQLLocalDB;initial catalog=FinancialApp; Integrated Security=True"
  },
  "JwtSettings": {
    "Key": "FINANCIAL_APP_SECRET_JWT_KEY_HERE",
    "Issuer": "FinancialApp_Api",
    "Audience": "FinancialAppUser",
    "DurationInMinutes": 60
  },
  "AllowedHosts": "*"
}
